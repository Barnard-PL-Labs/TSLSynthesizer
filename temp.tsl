always assume{
	!(play note62 && play note60);

}
always guarantee {
	[waveform <- sawtooth()] -> ((play note62 <-> [arpeggiatorOn <- True()]) W ![waveform <- sawtooth()]);
	[arpeggiatorStyle <- upStyle()] -> ((play note60 <-> [arpeggiatorStyle <- downStyle()]) W ![arpeggiatorStyle <- upStyle()]);
	[fmSynthesis <- False()] -> ((play note60 <-> [harmonizerOn <- True()]) W [fmSynthesis <- True()]);
	[waveform <- square()] -> ((play note62 <-> [lfo <- True()]) W ![waveform <- square()]);
}
